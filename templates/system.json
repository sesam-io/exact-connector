{
  "_id": "{{@ system @}}",
  "oauth2": {
    "access_token": "$SECRET(oauth_access_token)",
    "client_id": "$SECRET(oauth_client_id)",
    "client_secret": "$SECRET(oauth_client_secret)",
    "refresh_token": "$SECRET(oauth_refresh_token)",
    "refresh_window": 0,
    "token_url": "{{@ token_url @}}"
  },
  "operations": {
    "accounts-list": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "GET",
      "payload_property": "d.results",
      "url": "CRM/Accounts"
    },
    "accounts-lookup": {
      "headers": {
        "Accept": "application/json"
      },
      "method": "GET",
      "payload_property": "d.results",
      "url": "CRM/Accounts?$filter=ID eq guid'{{ entity.ID }}'"
    },
    "contacts-delete": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "DELETE",
      "url": "CRM/Contacts(guid'{{ entity.ID }}')"
    },
    "contacts-insert": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "POST",
      "payload_property": "d",
      "url": "CRM/Contacts"
    },
    "contacts-list": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "GET",
      "payload_property": "d.results",
      "url": "CRM/Contacts"
    },
    "contacts-lookup": {
      "headers": {
        "Accept": "application/json"
      },
      "method": "GET",
      "payload_property": "d.results",
      "url": "CRM/Contacts?$filter=ID eq guid'{{ entity.ID }}'"
    },
    "contacts-update": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "PUT",
      "url": "CRM/Contacts(guid'{{ entity.ID }}')"
    }
  },
  "type": "system:rest",
  "url_pattern": "https://start.exactonline.co.uk/api/v1/128132/%s",
  "verify_ssl": false
}