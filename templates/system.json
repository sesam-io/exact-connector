{
  "_id": "{{@ system @}}",
  "oauth2": {
    "access_token": "$SECRET(oauth_access_token)",
    "client_id": "$SECRET(oauth_client_id)",
    "client_secret": "$SECRET(oauth_client_secret)",
    "refresh_token": "$SECRET(oauth_refresh_token)",
    "refresh_window": -10,
    "token_url": "{{@ token_url @}}"
  },
  "operations": {
    "delete": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "DELETE",
      "url": "CRM/{{ properties.datatype }}(guid'{{ entity.ID }}')"
    },
    "insert": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "POST",
      "payload_property": "d",
      "url": "CRM/{{ properties.datatype }}"
    },
    "list": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "GET",
      "payload_property": "d.results",
      "url": "CRM/{{ properties.datatype }}"
    },
    "list_purchaseinvoices": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "GET",
      "payload_property": "d.results",
      "url": "purchase/{{ properties.datatype }}?$select=*"
    },
    "lookup": {
      "headers": {
        "Accept": "application/json"
      },
      "method": "GET",
      "payload_property": "d.results",
      "url": "CRM/{{ properties.datatype }}?$filter=ID eq guid'{{ entity.ID }}'"
    },
    "update": {
      "headers": {
        "Accept": "application/json"
      },
      "id_expression": "{{ ID }}",
      "method": "PUT",
      "url": "CRM/{{ properties.datatype }}(guid'{{ entity.ID }}')"
    }
  },
  "type": "system:rest",
  "url_pattern": "https://start.exactonline.co.uk/api/v1/128132/%s",
  "verify_ssl": false
}